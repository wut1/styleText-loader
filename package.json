{
  "name": "styletext-loader",
  "version": "1.0.2",
  "description": "style loader module for webpack",
  "main": "dist/cjs.js",
  "directories": {
    "test": "test"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "test": "npm run test:coverage",
    "test:coverage": "npm run test:only -- -u --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "test:only": "cross-env NODE_ENV=test jest -u",
    "test:manual": "npm run build && webpack-dev-server test/manual/src/index.js --open --config test/manual/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wut1/styleText-loader.git"
  },
  "author": "chex",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wut1/styleText-loader/issues"
  },
  "homepage": "https://github.com/wut1/styleText-loader#readme",
  "dependencies": {
    "loader-utils": "^1.2.3",
    "schema-utils": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "jest": "^24.9.0",
    "jsdom": "^16.5.0",
    "memfs": "^3.0.3",
    "webpack": "^4.41.5"
  },
  "keywords": [
    "webpack",
    "css string"
  ]
}
